def is_palindrome(s):
    if len(s) <= 1:
        return True
    if s[0] == s[-1]:
        return is_palindrome(s[1:-1])
    else:
        return False

# Contoh penggunaan pada soal
string_1= "racecar"
if is_palindrome(string_1):
    print(f"'{string_1}' adalah palindrom.")
else:
    print(f"'{string_1}' bukan palindrom.")

string_2 = "gohangasalamiimalasagnahog"
if is_palindrome(string_2):
    print(f"'{string_2}' adalah palindrom.")
else:
    print(f"'{string_2}' bukan palindrom.")
